package com.rsaame.pas.svc.cmn;

import com.mindtree.ruc.cmn.base.BaseVO;

public interface ISectionLoadDataSvc {
	/**
	 * Execute section DAO and fetch section data from table. The expected input is <code>LoadExistingInputVO</code>. The signature
	 * has been kept with BaseVO input and BaseVO return only to be able to comply with the needs of
	 * TaskExecutor.
	 * 
	 * @param input An instance of <code>LoadExistingInputVO</code>
	 * @return null
	 */
	
	public BaseVO loadSectionData( BaseVO input );

}
