package com.rsaame.pas.vo.app;

import java.math.BigDecimal;
import java.util.Date;

import com.mindtree.ruc.cmn.base.BaseVO;
import com.mindtree.ruc.cmn.reflect.IFieldValue;

public class ClaimsHistoryVO extends BaseVO implements IFieldValue{

	private static final long serialVersionUID = 1L;

	private String srNo;
	private long polPolicyNo;
	private String polPolicyType;
	private String polClassCode;
	private long intClaimNo;
	private String typeOfClaim;
	private String intELossDesc;
	private String causeOfLoss;
	private Date intDateOfLoss;
	private Date intDateOfIntimation;
	private BigDecimal claimAmount;
	private BigDecimal paidAmount;
	private BigDecimal claimOsAmount;
	private String comments;
	
	

	public Object getFieldValue( String fieldName ){
		Object fieldValue = null;

		if( "srNo".equals( fieldName ) ) fieldValue = getSrNo();
		if( "polPolicyNo".equals( fieldName ) ) fieldValue = getPolPolicyNo();         
		if( "polPolicyType".equals( fieldName ) ) fieldValue = getPolPolicyType();       
		if( "polClassCode".equals( fieldName ) ) fieldValue = getPolClassCode();     
		if( "intClaimNo".equals( fieldName ) ) fieldValue = getIntClaimNo();
		if( "typeOfClaim".equals( fieldName ) ) fieldValue = getTypeOfClaim();
		if( "intELossDesc".equals( fieldName ) ) fieldValue = getIntELossDesc();
		if( "causeOfLoss".equals( fieldName ) ) fieldValue = getCauseOfLoss();
		if( "intDateOfLoss".equals( fieldName ) ) fieldValue = getIntDateOfLoss();
		if( "intDateOfIntimation".equals( fieldName ) ) fieldValue = getIntDateOfIntimation(); 
		if( "claimAmount".equals( fieldName ) ) fieldValue = getClaimAmount();
		if( "paidAmount".equals( fieldName ) ) fieldValue = getPaidAmount();
		if( "claimOsAmount".equals( fieldName ) ) fieldValue = getClaimOsAmount();
		if( "comments".equals( fieldName ) ) fieldValue = getComments();
		return fieldValue;
	}

	public String getSrNo(){
		return srNo;
	}

	public void setSrNo( String srNo ){
		this.srNo = srNo;
	}

	public long getPolPolicyNo(){
		return polPolicyNo;
	}

	public void setPolPolicyNo( long polPolicyNo ){
		this.polPolicyNo = polPolicyNo;
	}

	public String getPolPolicyType(){
		return polPolicyType;
	}

	public void setPolPolicyType( String polPolicyType ){
		this.polPolicyType = polPolicyType;
	}

	public String getPolClassCode(){
		return polClassCode;
	}

	public void setPolClassCode( String polClassCode ){
		this.polClassCode = polClassCode;
	}

	public long getIntClaimNo(){
		return intClaimNo;
	}

	public void setIntClaimNo( long intClaimNo ){
		this.intClaimNo = intClaimNo;
	}

	public String getTypeOfClaim(){
		return typeOfClaim;
	}

	public void setTypeOfClaim( String typeOfClaim ){
		this.typeOfClaim = typeOfClaim;
	}

	public String getIntELossDesc(){
		return intELossDesc;
	}

	public void setIntELossDesc( String intELossDesc ){
		this.intELossDesc = intELossDesc;
	}

	public String getCauseOfLoss(){
		return causeOfLoss;
	}

	public void setCauseOfLoss( String causeOfLoss ){
		this.causeOfLoss = causeOfLoss;
	}

	public Date getIntDateOfLoss(){
		return intDateOfLoss;
	}

	public void setIntDateOfLoss( Date intDateOfLoss ){
		this.intDateOfLoss = intDateOfLoss;
	}

	public Date getIntDateOfIntimation(){
		return intDateOfIntimation;
	}

	public void setIntDateOfIntimation( Date intDateOfIntimation ){
		this.intDateOfIntimation = intDateOfIntimation;
	}

	public BigDecimal getClaimAmount(){
		return claimAmount;
	}

	public void setClaimAmount( BigDecimal claimAmount ){
		this.claimAmount = claimAmount;
	}

	public BigDecimal getPaidAmount(){
		return paidAmount;
	}

	public void setPaidAmount( BigDecimal paidAmount ){
		this.paidAmount = paidAmount;
	}

	public BigDecimal getClaimOsAmount(){
		return claimOsAmount;
	}

	public void setClaimOsAmount( BigDecimal claimOsAmount ){
		this.claimOsAmount = claimOsAmount;
	}

	public String getComments(){
		return comments;
	}

	public void setComments( String comments ){
		this.comments = comments;
	}

}
