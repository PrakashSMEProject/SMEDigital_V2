				        <name>criteria</name>
						<value>todayOrBefore</value>
				</parameter>
					<errorKey>pas.searchTransaction.transactionDateToValid</errorKey>
				</validation>
			</field>

			<beanValidation validatorClass="compareDatesForTransaction" />

		</bean>
	</transaction>
