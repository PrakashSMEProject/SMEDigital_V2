	<!-- Sample configuration -->
	<transaction name="SaveBasicInfo">
		<bean type="com.rsaame.pas.vo.bus.PolicyVO">
			<field name="generalInfo.insured.name">
				<validation type="mandatory">
					<errorKey>pas.name.required</errorKey>
				</validation>
				<!-- CR#-157510 Changes-Increasing Insured Name Limit -->
             <validation type="maxLength">
					<parameter>
						<name>length</name>
						<value>200</value>
					</parameter>
					<errorKey>pas.name.length</errorKey>
				</validation>

			<!-- CR#-157510 Changes-Increasing Insured Name Limit -->

				<!-- <validation type="allowedSpecialChars">
					<parameter>
						<name>charSets</name>
						<value>,._-</value>
					</parameter>
					<errorKey>pas.name.allowedspecialchars</errorKey>
				</validation> -->
			</field>

			<field name="generalInfo.insured.address.address">
				<validation type="mandatory">
					<errorKey>pas.address.required</errorKey>
				</validation>
				<validation type="maxLength">
					<parameter>
						<name>length</name>
						<value>200</value>
					</parameter>
					<errorKey>pas.address.length</errorKey>
				</validation>
			</field>

			<field name="generalInfo.insured.address.poBox">
				<validation type="mandatory">
					<errorKey>pas.pobox.required</errorKey>
				</validation>
				<validation type="maxLength">
					<parameter>
						<name>length</name>
						<value>30</value>
					</parameter>
					<errorKey>pas.pobox.length</errorKey>
				</validation>
			</field>

			<!-- Commented Emirates Field Validation as it has been removed from the 
				screen -->
			<!-- <field name="generalInfo.insured.address.emirates"> <validation type="mandatory"> 
				<errorKey>pas.emirates.required</errorKey> </validation> </field> <field 
				name="generalInfo.insured.address.country"> <validation type="mandatory"> 
				<errorKey>pas.country.required</errorKey> </validation> </field> -->

			<field name="generalInfo.insured.city">
				<validation type="mandatory">
					<errorKey>pas.city.required</errorKey>
				</validation>
			</field>

			<field name="generalInfo.insured.busType">
				<validation type="mandatory">
					<errorKey>pas.businesstype.required</errorKey>
				</validation>
			</field>

			<!-- <field name="insured.busDescription"> <validation type="mandatory"> 
				<errorKey>pas.businessdesc.required</errorKey> </validation> </field> <field 
				name="jurisdiction"> <validation type="mandatory"> <errorKey>pas.jurisdiction.required</errorKey> 
				</validation> </field> -->


			<!-- Distribution channel - server side validation is commented as distribution channel option
				will not be available for broker login
				<field name="generalInfo.sourceOfBus.distributionChannel">
					<validation type="mandatory">
						<errorKey>pas.distchannel.required</errorKey>
					</validation>
				</field>
			-->
		
			<field name="scheme.schemeCode">
				<validation type="mandatory">
					<errorKey>pas.schemecode.required</errorKey>
				</validation>
			</field>

			<field name="scheme.tariffCode">
				<validation type="mandatory">
					<errorKey>pas.tariff.required</errorKey>
				</validation>
			</field>

			<field name="scheme.policyCode">
				<validation type="mandatory">
					<errorKey>pas.policytype.required</errorKey>
				</validation>
			</field>

			<field name="policyTerm">
				<validation type="mandatory">
					<errorKey>pas.policyterm.required</errorKey>
				</validation>
			</field>
		</bean>
	</transaction>

