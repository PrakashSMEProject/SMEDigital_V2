				<parameter>
				        <name>criteria</name>
						<value>todayOrBefore</value>
				</parameter>
					<errorKey>pas.searchTransaction.transactionDateFromValid</errorKey>
				</validation>
			</field>

			<field name="transaction.transactionTo">
				<validation type="validDate">
