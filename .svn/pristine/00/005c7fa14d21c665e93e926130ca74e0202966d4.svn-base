package com.rsaame.pas.vo.bus;

import java.util.Date;

import com.mindtree.ruc.cmn.base.BaseVO;

public class AuthenticationInfoVO extends BaseVO{
	private static final long serialVersionUID = 1L;
	private Date accountingDate;
	private Integer txnType;
	private Date printedDate;
	private Integer licensedBy;
	private Integer approvedBy;
	private Date approvedDt;
	private Integer locationCode;
	private Integer intAccExecCode;
	private Integer extAccExecCode;
	private Long refPolicyNo;
	private Integer refPolicyYear;
	//added to map PolicyDataVo to TTrnpolicy
	private Long refPolicyId;
	private String renewalTerms;
	
	
	public Object getFieldValue( String fieldName ){
		Object fieldValue = null;



		if("accountingDate".equals(fieldName)) fieldValue = getAccountingDate();
		if("txnType".equals(fieldName)) fieldValue = getTxnType();
		if("printedDate".equals(fieldName)) fieldValue = getPrintedDate();
		if("licensedBy".equals(fieldName)) fieldValue = getLicensedBy();
		if("approvedBy".equals(fieldName)) fieldValue = getApprovedBy();
		if("approvedDt".equals(fieldName)) fieldValue = getApprovedDt();
		if("locationCode".equals(fieldName)) fieldValue = getLicensedBy();
		if("intAccExecCode".equals(fieldName)) fieldValue = getIntAccExecCode();
		if("extAccExecCode".equals(fieldName)) fieldValue = getExtAccExecCode();
		if("refPolicyNo".equals(fieldName)) fieldValue = getRefPolicyNo();
		if("refPolicyYear".equals(fieldName)) fieldValue = getRefPolicyYear();
		if("createdOn".equals(fieldName))fieldValue = getCreatedOn();
		if("refPolicyId".equals(fieldName)) fieldValue = getRefPolicyId();
		if("renewalTerms".equals(fieldName))fieldValue = getRenewalTerms();
		
		return fieldValue;
	}
	
	public String getRenewalTerms() {
		return renewalTerms;
	}

	public void setRenewalTerms(String renewalTerms) {
		this.renewalTerms = renewalTerms;
	}

	public Date getAccountingDate() {
		return accountingDate;
	}

	public void setAccountingDate(Date accountingDate) {
		this.accountingDate = accountingDate;
	}

	public Integer getTxnType() {
		return txnType;
	}

	public void setTxnType(Integer txnType) {
		this.txnType = txnType;
	}

	public Date getPrintedDate() {
		return printedDate;
	}

	public void setPrintedDate(Date printedDate) {
		this.printedDate = printedDate;
	}

	public Integer getLicensedBy() {
		return licensedBy;
	}

	public void setLicensedBy(Integer licensedBy) {
		this.licensedBy = licensedBy;
	}

	public Integer getApprovedBy() {
		return approvedBy;
	}

	public void setApprovedBy(Integer approvedBy) {
		this.approvedBy = approvedBy;
	}
	public Date getApprovedDt() {
		return approvedDt;
	}

	public void setApprovedDt(Date approvedDt) {
		this.approvedDt = approvedDt;
	}

	public Integer getLocationCode() {
		return locationCode;
	}

	public void setLocationCode(Integer locationCode) {
		this.locationCode = locationCode;
	}

	public Integer getIntAccExecCode() {
		return intAccExecCode;
	}

	public void setIntAccExecCode(Integer intAccExecCode) {
		this.intAccExecCode = intAccExecCode;
	}

	public Integer getExtAccExecCode() {
		return extAccExecCode;
	}

	public void setExtAccExecCode(Integer extAccExecCode) {
		this.extAccExecCode = extAccExecCode;
	}

	public Long getRefPolicyNo() {
		return refPolicyNo;
	}

	public void setRefPolicyNo(Long refPolicyNo) {
		this.refPolicyNo = refPolicyNo;
	}

	public Integer getRefPolicyYear() {
		return refPolicyYear;
	}

	public void setRefPolicyYear(Integer refPolicyYear) {
		this.refPolicyYear = refPolicyYear;
	}

	
	/**
	 * @return
	 */
	public Long getRefPolicyId() {
		return refPolicyId;
	}

	/**
	 * @param refPolicyId
	 */
	public void setRefPolicyId(Long refPolicyId) {
		this.refPolicyId = refPolicyId;
	}

	@Override
	public String toString() {
		return "AuthenticationInfoVO [accountingDate=" + accountingDate
				+ ", txnType=" + txnType + ", printedDate=" + printedDate
				+ ", licensedBy=" + licensedBy + ", approvedBy=" + approvedBy
				+ ", approvedDt=" + approvedDt + ", locationCode="
				+ locationCode + ", intAccExecCode=" + intAccExecCode
				+ ", extAccExecCode=" + extAccExecCode + ", refPolicyNo="
				+ refPolicyNo + ", refPolicyYear=" + refPolicyYear
				+ ", refPolicyId=" + refPolicyId + "]";
	}
	
	

}