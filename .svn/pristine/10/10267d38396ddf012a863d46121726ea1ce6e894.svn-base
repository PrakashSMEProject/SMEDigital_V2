package com.rsaame.pas.kaizen.vo;

import java.util.Map;

import com.rsaame.kaizen.framework.model.SecurityContext;

/**
 * @author m1014644
 *
 */
public class KaizenSecurityContextWrapper{
	

	/**
	 * @return the roleFunctionMap
	 */
	public static  Map<String, Map<String, String>> getRoleFunctionMap(){
		return SecurityContext.getRoleFunctionMap();
	}

	/**
	 * @param roleFunctionMap the roleFunctionMap to set
	 */
	public static void setRoleFunctionMap( Map<String, Map<String, String>> roleFunctionMap ){
		SecurityContext.setRoleFunctionMap(roleFunctionMap);
	}
	
	

}
