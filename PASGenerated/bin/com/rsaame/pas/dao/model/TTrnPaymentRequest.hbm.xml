<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.rsaame.pas.dao.model.TTrnPaymentRequest" table="T_TRN_PAYMENT_REQUEST">
		<id name="pdrRefNo" type="java.lang.String" >
			<column name="PDR_REF_NO" length="200" />
		</id>
			
		<property name="pdrQuoteNo" type="java.lang.Long" >
			<column name="PDR_QUOTE_NO" precision="16" scale="0" />
		</property>

		<property name="pdrPolicyId" type="java.lang.Long">
			<column name="PDR_POLICY_ID" precision="16" scale="0" />
		</property>
		
		<property name="pdrEndtId" type="java.lang.Long">
			<column name="PDR_ENDT_ID" precision="16" scale="0" />
		</property>
		
		
		<property name="pdrCustName" type="java.lang.String">
			<column name="PDR_CUST_NAME" length="200" />
		</property>
		
		<property name="pdrCustMobile" type="java.lang.String">
			<column name="PDR_CUST_MOBILE" length="30" />
		</property>
		
		<property name="pdrCustEmailId" type="java.lang.String">
			<column name="PDR_CUST_EMAIL_ID" length="30" />
		</property>
		
			<property name="pdrTransAmount" type="java.math.BigDecimal">
			<column name="PDR_TRANS_AMOUNT" precision="16" scale="0" />
		</property>
		
		<property name="pdrTarCode" type="java.lang.Integer">
			<column name="PDR_TAR_CODE" precision="6" scale="0" />
		</property>
		
		<property name="pdrDocumentCode" type="java.lang.Short">
			<column name="PDR_DOCUMENT_CODE" precision="4" scale="0" />
		</property>
		
		<property name="pdrTransDate" type="java.util.Date">
			<column name="PDR_TRANS_DATE" length="7" />
		</property>
		
		<property name="pdrRequest" type="java.lang.String">
			<column name="PDR_REQUEST" length="4000" />
		</property>
		
			<property name="pdrBrokerCode" type="java.lang.Integer">
			<column name="PDR_BROKER_CODE" precision="4" scale="0" />
		</property>
		
	</class>

</hibernate-mapping>